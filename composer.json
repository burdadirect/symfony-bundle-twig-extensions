  "description": "HBM Twig Extensions Bundle - Datetime, Object, String helper for twig.",
  "authors" : [{
    "name" : "Christian Puchinger",
    "email" : "christian.puchinger@burda.com"
  }],
  "license": "MIT",
  "type": "symfony-bundle",
  "autoload": {
    "psr-0": {
      "HBM\\TwigExtensionsBundle": ""
    }
  },
  "require": {
    "php": ">=7.1",
    "ext-dom": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "symfony/config": "^3.4|^4.2",
    "symfony/dependency-injection": "^3.4|^4.2",
    "symfony/dom-crawler": "^3.4|^4.2",
    "symfony/http-kernel": "^3.4|^4.2",
    "symfony/monolog-bridge": "^3.4|^4.2",
    "symfony/twig-bundle": "^3.4|^4.2"

